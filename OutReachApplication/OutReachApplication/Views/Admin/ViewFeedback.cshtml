@model IEnumerable<OutReachApplication.Models.Feedback>

@{
    ViewBag.Title = "ViewFeedback";
    Layout = "~/Views/Shared/_CommonLayout.cshtml";
}




<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.VolunteerId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Are_you_satisfied_for_this_Event)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Have_you_faced_any_difficulties_in_this_Event)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Do_you_think_the_duration_of_the_event_was_good_enough_as_per_your_expectation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Did_the_event_content_and_delivery_meet_your_expectation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Do_you_like_to_recommand_this_event_to_someone)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.VolunteerId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Are_you_satisfied_for_this_Event)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Have_you_faced_any_difficulties_in_this_Event)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Do_you_think_the_duration_of_the_event_was_good_enough_as_per_your_expectation)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Did_the_event_content_and_delivery_meet_your_expectation)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Do_you_like_to_recommand_this_event_to_someone)
        </td>
       
    </tr>
}

</table>

<div>
    @Html.ActionLink("Back", "AdminIndex")
</div>